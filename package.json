    "last-fm",
    "lastfm",
    "nodejs",
    "api",
    "node",
    "typescript"
  ],
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "cz-conventional-changelog": "^3.3.0",
    "rimraf": "^4.3.1",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
