{
  "name": "lastfm-client-api",
  "version": "1.0.2",
  "description": "A LastFM client written in Typescript",
  "main": "./dist",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "clean": "rimraf dist",
    "dev": "npm run clean && tsc --watch --project tsconfig.dev.json"
  },
  "repository": "https://github.com/ansango/lastfm-api-client.git",
  "author": "Anibal Santos",
  "license": "MIT",
  "keywords": [
    "client",
    "lastFm",
    "nodejs",
    "typescript",
    "node"
  ],
  "devDependencies": {
    "@types/node": "^18.14.6",
    "cross-fetch": "^3.1.5",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "rimraf": "^4.3.1",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
