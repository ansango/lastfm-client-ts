{
  "name": "lastfm-client-ts",
  "version": "1.0.4-alpha.2",
  "description": "A LastFM client written in Typescript",
  "main": "./dist",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "clean": "rimraf dist",
    "dev": "npm run clean && tsc --watch --project tsconfig.dev.json"
